{
  "name": "adam-bot-backend",
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "build": "tsc && tsc-alias && npm prune --production",
    "register:commands": "ts-node -r tsconfig-paths/register src/register-commands.ts",
    "dev": "cross-env NODE_ENV=development ts-node-dev --respawn --transpile-only -r tsconfig-paths/register src/index.ts",
    "db:generate": "npx drizzle-kit generate:pg --schema ./src/db/schema.ts --out ./src/db/migrations",
    "db:migrate": "node run src/db/migrations.ts"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "discord.js": "^14.19.3",
    "dotenv": "^16.3.1",
    "drizzle-orm": "^0.44.1",
    "express": "^4.18.2",
    "pg": "^8.16.0",
    "pg-pool": "^3.10.0"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.17.47",
    "@types/pg": "^8.15.2",
    "cross-env": "^7.0.3",
    "drizzle-kit": "^0.31.1",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3"
  }
}
  